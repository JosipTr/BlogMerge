<%- include("../includes/head.ejs")  %>
<link rel="stylesheet" href="/css/post-list.css">
</head>
<%- include("../includes/header.ejs")  %>
<section class="post-list">
  <h2>Public posts</h2>

  <% posts.forEach(post => { %>
  <div class="post-item">
    <a href="/blog/<%= post._id %>"><%= post.title %></a> by <%= post.user.email %>
  </div>
  <% }); %>
</section>
<%- include("../includes/footer.ejs")  %>